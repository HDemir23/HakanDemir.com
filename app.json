{
  "expo": {
    "name": "Cv",
    "slug": "Cv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-512x512.png",
    "scheme": "cv",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-512x512.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.roseprince.Cv"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon-512x512.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-512x512.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "990033a2-d66f-4aca-bd1f-c67f264496a9"
      }
    }
  }
}
